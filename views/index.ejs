<% include _layouts/header %>

    <div id="navigation">
        <!-- container -->
        <div class="container">
            <div id="responsive-nav">
                <!-- category nav -->
                <div class="category-nav">
                    <span class="category-header">

                    <%- messages('messages', locals) %>

                    <% if (errors) {%>
                    <% errors.forEach(function(error) { %>
                    <div class="alert alert-danger">
                        <%= error.msg %>
                    </div>
                    <% });%>
                    <% }%>

                    Categories <i class="fa fa-list"></i></span>
                    <ul class="category-list">
                        
                        <% categories.forEach(function(c){ %>
                            <li>
                                <a href="/products/<%= c.slug %>">
                                    <%= c.title %>
                                </a>
                            </li>
                            <% }); %>
                    </ul>
                </div>
                <!-- /category nav -->

                <!-- menu nav -->
                <div class="menu-nav">
                    <span class="menu-header">Menu <i class="fa fa-bars"></i></span>
                    <ul class="menu-list">
                        
                    </ul>
                </div>
                <!-- menu nav -->
            </div>
        </div>
        <!-- /container -->
    </div>

    <div id="home">
        <!-- container -->
        <div class="container">
            <!-- home wrap -->
            <div class="home-wrap">
                <!-- home slick -->
                <div id="home-slick" class="slick-initialized slick-slider"><button class="slick-prev slick-arrow" aria-label="Previous" type="button" style="display: block;">Previous</button>
                    <!-- banner -->
                    <div class="slick-list draggable">
                        <div class="slick-track" style="opacity: 1; width: 6090px; transform: translate3d(-3480px, 0px, 0px); transition: transform 300ms ease 0s;">
                            <div class="banner banner-1 slick-slide slick-cloned" data-slick-index="-1" aria-hidden="true" tabindex="-1" style="width: 870px;">
                                <img src="/img/uniosun1.jpg" alt="">
                            </div>
                            <div class="banner banner-1 slick-slide slick-current slick-active" data-slick-index="0" aria-hidden="false" tabindex="-1" style="width: 870px;">
                                <img src="/img/uniosun1.jpg" alt="">
                            </div>
                            <div class="banner banner-1 slick-slide" data-slick-index="1" aria-hidden="true" tabindex="-1" style="width: 870px;">
                                <img src="/img/uniosun1.jpg" alt="">
                            </div>
                            <div class="banner banner-1 slick-slide" data-slick-index="2" aria-hidden="true" tabindex="0" style="width: 870px;">
                                <img src="/img/uniosun1.jpg" alt="">
                            </div>
                            <div class="banner banner-1 slick-slide slick-cloned" data-slick-index="3" aria-hidden="true" tabindex="-1" style="width: 870px;">
                                <img src="/img/uniosun1.jpg" alt="">

                            </div>
                        </div>
                    </div>
                    <!-- /banner -->

                    <!-- banner -->

                    <!-- /banner -->

                    <!-- banner -->

                    <!-- /banner -->
                    <button class="slick-next slick-arrow" aria-label="Next" type="button" style="display: block;">Next</button>
                </div>
                <!-- /home slick -->
            </div>
            <!-- /home wrap -->
        </div>
        <!-- /container -->
    </div>

    <div class="section">
        <!-- container -->
        <div class="container">
            <!-- row -->
            <div class="row">
                <!-- banner -->
                <div class="col-md-4 col-sm-6">
                    <a class="banner banner-1" href="#">
                        <img src="./img/img1.jpg" alt="">
                        <div class="banner-caption text-center">
                            <h2 class="white-color">Oke-Baale</h2>
                        </div>
                    </a>
                </div>
                <!-- /banner -->

                <!-- banner -->
                <div class="col-md-4 col-sm-6">
                    <a class="banner banner-1" href="#">
                        <img src="./img/img2.jpg" alt="">
                        <div class="banner-caption text-center">
                            <h2 class="white-color">Olaiya</h2>
                        </div>
                    </a>
                </div>
                <!-- /banner -->

                <!-- banner -->
                <div class="col-md-4 col-md-offset-0 col-sm-6 col-sm-offset-3">
                    <a class="banner banner-1" href="#">
                        <img src="./img/img3.jpg" alt="">
                        <div class="banner-caption text-center">
                            <h2 class="white-color">Estate</h2>
                        </div>
                    </a>
                </div>
                <!-- /banner -->

            </div>
            <!-- /row -->

            <div class="col-sm-12 section section-grey">
                <%- content %>




            </div>

            <div class="row">
                <!-- section title -->
                <div class="col-md-12">
                    <div class="section-title">
                        <h2 class="title">Markets Nearest to You</h2>
                    </div>
                </div>
                <!-- section title -->

                <!-- Product Single -->
                <% products.forEach(function(product){ %>
                    <div class="col-md-3 col-sm-6 col-xs-6">

                        <div class="product product-single">
                            <div class="product-thumb">
                                <img src="<%= product.image %>" alt="">
                            </div>
                            <div class="product-body">
                                
                                <div class="product-rating">
                                    <i class="fa fa-star"></i>
                                    <i class="fa fa-star"></i>
                                    <i class="fa fa-star"></i>
                                    <i class="fa fa-star"></i>
                                    <i class="fa fa-star-o empty"></i>
                                </div>
                                
                                <!-- <h2 class="product-name">
                                    <a href="#">
                                        <%= product.category %>
                                    </a>
                                </h2>
                                <h2 class="product-name"><a href="#">Product Name Goes Here</a></h2> -->
                                <a class="vd" href="/products/<%=product.category%>/<%=product.slug %>">View details</a>
                                <h2 class="product-name">
                                    
                                        <%= product.location %>
                                    </a>
                                </h2>
                            </div>
                        </div>
                    </div>
                <% }) %>

                <!-- /Product Single -->


            </div>

        </div>
        <!-- /container -->
    </div>




    <% include _layouts/footer %>